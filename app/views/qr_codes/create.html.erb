<h1><%=@user.first_name%>&nbsp<%=@user.last_name%><h1>
<div class="membership_image"><%= image_tag @user.user_image, width:100 %></div>
<table class="qr-code">
<% @qr.modules.each_index do |x| %>
  <tr>
  <% @qr.modules.each_index do |y| %>
   <% if @qr.dark?(x,y) %>
    <td class="black"/>
   <% else %>
    <td class="white"/>
   <% end %>
  <% end %>
  </tr>
<% end %>
</table>
<br />
<%= @expiry %>


